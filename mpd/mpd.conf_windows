gapless_mp3_playback "no"
zeroconf_enabled "no"
mixer_type "disabled"
audio_buffer_size "3684"
buffer_before_play "9.%"
input {
plugin "ffmpeg"
enabled "no"
}
input {
plugin "nfs"
enabled "no"
}
decoder {
plugin "vorbis"
enabled "no"
}
decoder {
plugin "oggflac"
enabled "no"
}
decoder {
plugin "opus"
enabled "no"
}
decoder {
plugin "pcm"
enabled "no"
}
music_directory "d:/Music"
playlist_directory "c:/mpd/playlists"
db_file	"c:/mpd/database"
audio_output {
type "winmm"
name "winmm"
mixer_type "none"
always_on "yes"
tags "no"
replay_gain_handler "none"
}
