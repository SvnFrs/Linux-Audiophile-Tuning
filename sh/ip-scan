for i in 198.144.156. 199.21.149.; do
 for j in $(seq 0 255); do
  ip="$i$j"
  printf "\33[2K\rðŸ–§  $ip"
  if ping -w2 -c1 $ip >/dev/null; then
  #url="http://$ip:9083/live/CA42-live1/4U/m7/4Um7jSUcj5yAeervICdrrA==/live.m3u8"
   url="http://$ip:9083/live/CA42-live1/RQ/os/RQospRQpKr9SkY8DTXe_mw==/live.m3u8"
   curl -m2 -s "$url" | grep -qs 'EXTM3U' && printf "\n"
  fi
 done
done

