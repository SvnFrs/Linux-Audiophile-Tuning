1. sudo nano /etc/sysctl.conf
	 kernel.sched_rt_period_us = 10000000
	 kernel.sched_rt_runtime_us = 8116658
	 kernel.sched_wakeup_granularity_ns = 30000000
	 kernel.sched_latency_ns = 120000
	#kernel.sched_cfs_bandwidth_slice_us = 5000
	 kernel.sched_nr_migrate = 0
	#kernel.sched_rr_timeslice_ms = 0
	 kernel.sched_shares_window_ns = 3000000

	For Armbian
		sudo nano /etc/sysctl.conf
			kernel.sched_rt_runtime_us = -1
			kernel.sched_rt_period_us = 8543852
		sudo nano /etc/rc.local
			sysctl -w kernel.sched_rt_runtime_us=8116658
